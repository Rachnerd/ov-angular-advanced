    "@storybook/addon-links": "^8.4.2",
    "@storybook/angular": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.4",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "reset-css": "^5.0.1",
    "storybook": "^8.4.2",
    "typescript": "~5.4.5"
  }
